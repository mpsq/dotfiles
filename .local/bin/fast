#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'fast.com==1.0.2','console_scripts','fast'
__requires__ = 'fast.com==1.0.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('fast.com==1.0.2', 'console_scripts', 'fast')()
    )
