[Unit]
Description=Audio effects for PipeWire applications
PartOf=graphical-session.target
After=pipewire.service
Wants=pipewire.service

[Service]
Type=simple
Restart=on-failure
RestartSec=3
ExecStart=/usr/bin/easyeffects --gapplication-service
Slice=session.slice

[Install]
WantedBy=default.target
